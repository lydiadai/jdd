  <div class="container">
<table class="table table-bordered">
  <% @orders.each do |order| %>
  <thead>
    <tr>
      <td>#</td>
      <td>订单名称</td>
      <td>购买人</td>
      <td>创建时间</td>
      <td>付款状态</td>
      <td>订单状态</td>
      <td>操作</td>
    </tr>
  </thead>
  <tr>
    <td><%= order.id %></td>
    <td><%= link_to(order.token,order_path(order.token)) %></td>
    <td><%= order.user.email %></td>
    <td><%= order.created_at %></td>
    <td><%= order.is_paid %></td>
    <td><%= render "admin/orders/state_option",order: @order %></td>
    <td class="text-center">
      <%= link_to account_order_path(order.id),method: :delete do %>
      <i class="fa fa-trash"></i>
      <% end %>
    </td>
  </tr>
  <% end %>
</table>
</div>
